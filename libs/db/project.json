{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bitflags/**/*.ts"]
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma format",
        "cwd": "libs/db"
      }
    },
    "push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db push --accept-data-loss",
        "cwd": "libs/db"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate",
        "cwd": "libs/db"
      }
    },
    "seed-build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/libs/db/",
        "outputFileName": "seed.js",
        "main": "libs/db/src/seed.ts",
        "tsConfig": "libs/db/tsconfig.lib.json"
      },
      "configurations": {
        "development": {}
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "dependsOn": ["seed-build"],
      "options": {
        "command": "node dist/libs/db/seed.js --reset --s3files"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio",
        "cwd": "libs/db"
      }
    }
  },
  "tags": []
}
