{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/db/**/*.ts"]
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma format",
        "cwd": "libs/db/src/prisma"
      }
    },
    "push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db push --accept-data-loss",
        "cwd": "libs/db/src/prisma"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate",
        "cwd": "libs/db/src/prisma"
      }
    },
    "build-scripts": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "platform": "node",
        "outputPath": "dist/libs/db/scripts",
        "main": "libs/db/src/scripts/seed.ts",
        "tsConfig": "libs/db/tsconfig.scripts.json",
        "bundle": true,
        "format": ["cjs"],
        "additionalEntryPoints": ["libs/db/src/scripts/reset.ts"]
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-scripts"],
      "options": {
        "command": "node dist/libs/db/scripts/seed.cjs --reset --s3files"
      }
    },
    "reset": {
      "executor": "nx:run-commands",
      "dependsOn": ["build-scripts"],
      "options": {
        "command": "node dist/libs/db/scripts/reset.cjs"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio",
        "cwd": "libs/db/src/prisma"
      }
    }
  },
  "tags": []
}
