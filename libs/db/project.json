{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/db",
        "main": "libs/db/src/index.ts",
        "tsConfig": "libs/db/tsconfig.lib.json",
        "assets": ["libs/db/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/db/**/*.ts"]
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma format",
        "cwd": "libs/db/src"
      }
    },
    "push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db push --accept-data-loss",
        "cwd": "libs/db/src"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --no-hints",
        "cwd": "libs/db/src"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy",
        "cwd": "libs/db/src"
      }
    },
    "create-migration": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --skip-seed --name",
        "cwd": "libs/db/src",
        "parallel": false
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run scripts:seed",
        "cwd": "."
      }
    },
    "reset": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma db push --force-reset --accept-data-loss",
        "cwd": "libs/db/src"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio",
        "cwd": "libs/db/src"
      }
    }
  }
}
