{
    "name": "momentum-mod-api",
    "private": true,
    "version": "1.0.0",
    "description": "Momentum Mod API",
    "license": "MIT",
    "scripts": {
        "env": "dotenv -e ../.env",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start:seed": "npm run seed && npm run start",
        "start:push": "npm run push && npm run start",
        "start:migrate:debug": "npm run migrate && npm run start:debug",
        "start:push:debug": "npm run push && npm run start:debug",
        "start:seed:debug": "npm run push && npm run start:debug",
        "start:migrate:prod": "dotenv -e ../.env -- prisma migrate dev --name production_deploy && npm run start:prod",
        "push": "dotenv -e ../.env -- prisma db push --accept-data-loss",
        "generate": "dotenv -e ../.env -- prisma generate",
        "migrate": "dotenv -e ../.env -- prisma migrate dev --name init --skip-seed",
        "reset": "dotenv -e ../.env -- prisma migrate reset --force --skip-generate --skip-seed",
        "seed": "dotenv -e ../.env -- npm run migrate && dotenv -e ../.env -- prisma db seed",
        "test": "jest --config tests/e2e/e2e.config.ts --runInBand",
        "test:push": "npm run push && jest --config tests/e2e/e2e.config.ts --runInBand",
        "test:reset": "npm run reset && jest --config tests/e2e/e2e.config.ts --runInBand",
        "test:watch": "jest --config tests/e2e/e2e.config.ts --watch --runInBand",
        "test:cov": "jest --coverage --runInBand",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "lint": "eslint \"{src,tests,lib,config}/**/*.ts\" --fix",
        "lint:check": "eslint \"{src,tests,lib,config}/**/*.ts\"",
        "format": "prettier --write \"{src,lib,tests,config}/**/*.{ts,js,md}\"",
        "format:check": "prettier --check \"{src,lib,tests,config}/**/*.{ts,js,md}\"",
        "quality": "npm run lint && npm run format",
        "quality:check": "npm run lint:check && npm run format:check",
        "prepare": "cd .. && husky install"
    },
    "jest": {
        "testTimeout": 15000
    },
    "prisma": {
        "seed": "ts-node -r tsconfig-paths/register prisma/scripts/seed.ts"
    },
    "lint-staged": {
        "{src,lib,tests,config}/**/*.{ts,js,md}": [
            "eslint --quiet --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.287.0",
        "@nestjs/axios": "2.0.0",
        "@nestjs/common": "^9.3.9",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.3.9",
        "@nestjs/jwt": "^10.0.2",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.3.9",
        "@nestjs/swagger": "^6.2.1",
        "@nestjs/terminus": "^9.2.1",
        "@prisma/client": "^4.11.0",
        "@sentry/node": "^7.41.0",
        "@sentry/tracing": "^7.41.0",
        "@types/passport-steam": "^1.0.1",
        "@types/sharp": "^0.31.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "dotenv-cli": "^7.0.0",
        "lodash": "^4.17.21",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "passport-steam": "^1.0.17",
        "prom-client": "^14.2.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^4.4.0",
        "rxjs": "^7.8.0",
        "sharp": "^0.31.3",
        "steam-appticket": "^1.0.1",
        "swagger-ui-express": "^4.6.2",
        "type-fest": "^3.6.1"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@mermaid-js/mermaid-cli": "^10.0.2",
        "@nestjs/cli": "^9.2.0",
        "@nestjs/schematics": "^9.0.4",
        "@nestjs/testing": "^9.3.9",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.4.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.14.6",
        "@types/passport-jwt": "^3.0.8",
        "@types/passport-local": "^1.0.35",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "axios": "^1.3.4",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unicorn": "^46.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-node": "^29.5.0",
        "lint-staged": "^13.1.2",
        "prettier-eslint": "^15.0.1",
        "prettier-plugin-prisma": "^4.10.0",
        "prisma": "^4.11.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5"
    }
}
