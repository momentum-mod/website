{
    "name": "momentum-mod-api",
    "private": true,
    "version": "1.0.0",
    "description": "Momentum Mod API",
    "license": "MIT",
    "scripts": {
        "env": "dotenv -e ../.env",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start:seed": "npm run seed && npm run start",
        "start:push": "npm run push && npm run start",
        "start:migrate:debug": "npm run migrate && npm run start:debug",
        "start:push:debug": "npm run push && npm run start:debug",
        "start:seed:debug": "npm run push && npm run start:debug",
        "start:migrate:prod": "dotenv -e ../.env -- prisma migrate dev --name production_deploy && npm run start:prod",
        "push": "dotenv -e ../.env -- prisma db push --accept-data-loss",
        "migrate": "dotenv -e ../.env -- prisma migrate dev --name init --skip-seed",
        "reset": "dotenv -e ../.env -- prisma migrate reset --force --skip-generate --skip-seed",
        "seed": "dotenv -e ../.env -- npm run migrate && dotenv -e ../.env -- prisma db seed",
        "test": "jest --config tests/e2e/e2e.ts",
        "test:push": "npm run push && jest --config tests/e2e/e2e.ts",
        "test:reset": "npm run reset && jest --config tests/e2e/e2e.ts",
        "test:watch": "jest --config tests/e2e/e2e.ts --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "lint": "eslint \"{src,tests,lib,config}/**/*.ts\" --fix",
        "lint:check": "eslint \"{src,tests,lib,config}/**/*.ts\"",
        "format": "prettier --write \"{src,lib,tests,config}/**/*.ts\"",
        "format:check": "prettier --check \"{src,lib,tests,config}/**/*.ts\"",
        "prepare": "cd .. && husky install server/.husky",
        "pre-commit": "lint-staged"
    },
    "jest": {
        "testTimeout": 15000
    },
    "prisma": {
        "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
    },
    "lint-staged": {
        "{src,lib,tests,config}/**/*.ts": [
            "eslint --quiet --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.213.0",
        "@nestjs/axios": "1.0.0",
        "@nestjs/common": "^9.2.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.2.0",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.2.0",
        "@nestjs/swagger": "^6.1.3",
        "@nestjs/terminus": "^9.1.3",
        "@prisma/client": "^4.6.1",
        "@sentry/node": "^7.20.0",
        "@sentry/tracing": "^7.20.0",
        "@types/passport-steam": "^1.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "dotenv-cli": "^6.0.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-steam": "^1.0.17",
        "prom-client": "^14.1.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.7",
        "steam-appticket": "^1.0.1",
        "swagger-ui-express": "^4.6.0",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@mermaid-js/mermaid-cli": "^9.2.2",
        "@nestjs/cli": "^9.1.5",
        "@nestjs/schematics": "^9.0.3",
        "@nestjs/testing": "^9.2.0",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.2.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.11.9",
        "@types/passport-jwt": "^3.0.7",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "axios": "^1.1.3",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unicorn": "^44.0.2",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-environment-node": "^29.3.1",
        "lint-staged": "^13.0.3",
        "prettier-eslint": "^15.0.1",
        "prisma": "^4.6.1",
        "prisma-erd-generator": "^1.2.4",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.1",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.0",
        "typescript": "^4.9.3"
    }
}
