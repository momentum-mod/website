{
    "name": "momentum-mod-api",
    "private": true,
    "version": "1.0.0",
    "description": "Momentum Mod API",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start:migrate:debug": "npm run seed && npm run start:debug",
        "start:migrate:prod": "prisma migrate dev --name production_deploy && npm run start:prod",
        "seed": "prisma migrate dev --name init && prisma db seed",
        "reset": "prisma migrate reset --force --skip-seed",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "npm run reset && jest --config ./tests/jest-e2e.json",
        "test:e2e:watch": "npm run reset && jest --config ./tests/jest-e2e.json --watch"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.100.0",
        "@nestjs/axios": "0.0.8",
        "@nestjs/common": "^8.4.6",
        "@nestjs/core": "^8.4.6",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.4.6",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/terminus": "^8.0.6",
        "@prisma/client": "^3.15.0",
        "@sentry/node": "^7.0.0",
        "@sentry/tracing": "^7.0.0",
        "@types/passport-steam": "^1.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "passport": "^0.5.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-steam": "^1.0.17",
        "prom-client": "^14.0.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.5",
        "steam-appticket": "^1.0.1",
        "swagger-ui-express": "^4.4.0",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.1.0",
        "@mermaid-js/mermaid-cli": "^9.1.2",
        "@nestjs/cli": "^8.2.6",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.6",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.1",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.40",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^28.1.0",
        "jest-environment-node": "^28.1.0",
        "prettier-eslint": "^15.0.1",
        "prisma": "^3.15.0",
        "prisma-erd-generator": "^0.11.4",
        "source-map-support": "^0.5.21",
        "supertest": "^6.2.3",
        "ts-jest": "^28.0.4",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.3"
    }
}
